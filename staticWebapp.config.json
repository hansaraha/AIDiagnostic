{
    "navigationFallback": {
      "rewrite": "/index.html"
    },
    "routes": [
      {
        "route": "/*.*",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      },
      {
        "route": "/*",
        "headers": {
          "Cache-Control": "no-store"
        }
      }
    ],
    "globalHeaders": {
      "content-type": "text/html"
    },
    "mimeTypes": {
      ".js": "text/javascript",
      ".css": "text/css",
      ".json": "application/json"
    }
  }
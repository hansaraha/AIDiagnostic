{
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/images/*.{png,jpg,gif,svg}", "/css/*", "/_nuxt/*"]
    },
    "routes": [
      {
        "route": "/_nuxt/*",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      },
      {
        "route": "/*",
        "headers": {
          "Cache-Control": "no-cache"
        }
      }
    ],
    "responseOverrides": {
      "404": {
        "rewrite": "/index.html",
        "statusCode": 200
      }
    },
    "globalHeaders": {
      "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval'; img-src 'self' data:; font-src 'self' data:; connect-src 'self' https://*;"
    },
    "trailingSlash": "never"
  }